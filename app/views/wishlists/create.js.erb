
<% if @wishlist.errors.any? %>
  // Render new wishlist form with errors
  console.log('error')
<% else %>
  // Create a paragraph for the new wishlist. But hide it.
  var new_wishlist = $("<%= j render 'shared/wishpopline', wishlist: @wishlist, place: @place %>");

  // Add it to the DOM, at the end of the existing wishlists. It's still hidden.
  $('.wishlist-list').append(new_wishlist);



  $('input#wishlist_name').val('');

<% end %>